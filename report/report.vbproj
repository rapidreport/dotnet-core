<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>jp.co.systembase.report</RootNamespace>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <PackageId>jp.co.systembase.report</PackageId>
    <Authors>systembase</Authors>
    <Company>systembase</Company>
    <Version>5.12</Version>
    <AssemblyName>systembase.report</AssemblyName>
    <Product>jp.co.systembase.report</Product>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>report.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>5.12.0.0</AssemblyVersion>
    <OptionCompare>Text</OptionCompare>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\bin\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="component\textsplitter\" />
    <Folder Include="customizer\" />
    <Folder Include="data\internal\" />
    <Folder Include="elementpreprocessor\" />
    <Folder Include="expression\" />
    <Folder Include="method\" />
    <Folder Include="operator\" />
    <Folder Include="renderer\" />
    <Folder Include="scanner\" />
    <Folder Include="search\searchobject\" />
    <Folder Include="textformatter\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="5.0.0" />
  </ItemGroup>

</Project>
